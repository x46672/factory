                    <div class="row">
                        <div class="col-md-4">
                            <input class="fullwidth" id="contact_name" name=
                            "contact_name" placeholder="NAME *" required=""
                            title="Name" type="text" value="">
                        </div>

                        <div class="col-md-4">
                            <input class="fullwidth" id="contact_email" name=
                            "contact_email" placeholder="EMAIL *" required=""
                            title="Email" type="email" value="">
                        </div>

                        <div class="col-md-4">
                            <input class="fullwidth" id="contact_subject" name=
                            "contact_subject" placeholder="SUBJECT" title=
                            "subject" type="text" value="">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <textarea class="fullwidth" id="contact_comment"
                            name="contact_comment" placeholder="MESSAGE *"
                            required="" rows="5">
                            </textarea>
                        </div>

                        <div class="col-md-12">
                            <input id="captcha" name="captcha" type="hidden"
                            value="">
                            <!-- hidden field - humans should not see it, only spam bots will see it -->
                             <button class="btn btn-default" id=
                            "contact_submit">SEND MESSAGE</button>
                        </div>
                    </div>
                </form>
            </article>
        </div>
